  },
  "devDependencies": {
    "@types/command-line-args": "^5.2.0",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.16.1",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
